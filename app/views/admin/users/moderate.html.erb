<h1>Liste des dernières photos de profils ajoutées</h1>

<table class="table">
  <th>id</th>
  <th>name</th>
  <th>avatar</th>
  <th></th>

  <% @users.each do |user| %>
    <% presenter = UserPresenter.new(user: user) %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.first_name %> <%= user.last_name %> </td>
      <td><%= presenter.avatar %> </td>
      <td><%= presenter.destroy_avatar_link %></td>
      <td><%= presenter.validation_status_action_link %></td>
    </tr>
  <% end %>
</table>

<%= paginate @users %>
